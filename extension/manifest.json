{
  "manifest_version": 2,
  "name": "frost hack",
  "description": "frost hack",
  "version": "1.0",
  "permissions": [
    "cookies",
    "storage",
    "downloads",
    "activeTab",
    "tabs",
    "https://meet.google.com/*"
  ],
  "browser_action": {
    "default_popup": "./html/popup.html"
  },
  "background": {
    "scripts": [
      "./javascript/axios.js",
      "./javascript/socket.io.js",
      "./javascript/background.js"
    ],
    "persistent": false
  }
}
